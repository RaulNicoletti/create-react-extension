{
  "manifest_version": 2,
  "name": "Format PR",
  "version": "1.0",
  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "devtools_page": "devtools.html",
  "background": {
    "scripts": ["static/js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["static/js/contentScripts.js"]
    }
  ]
}
